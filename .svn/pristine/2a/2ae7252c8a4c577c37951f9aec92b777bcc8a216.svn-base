package com.smht.service.core.configuration;

import java.util.ArrayList;
import java.util.List;

public class WorkerInfo {
	private int count = 1;
	
	private int threadPoolCapacity = 1;
	
	private List<ServerInfo> serverInfos = new ArrayList<ServerInfo>();
	
	private List<FunctionInfo> actions = new ArrayList<FunctionInfo>();

	public int getCount() {
		return count;
	}

	public void setCount(int count) {
		this.count = count;
	}

	public int getThreadPoolCapacity() {
		return threadPoolCapacity;
	}

	public void setThreadPoolCapacity(int threadPoolCapacity) {
		this.threadPoolCapacity = threadPoolCapacity;
	}

	public List<FunctionInfo> getActions() {
		return actions;
	}

	public void setActions(List<FunctionInfo> actions) {
		this.actions = actions;
	}

	public List<ServerInfo> getServerInfos() {
		return serverInfos;
	}

	public void setServerInfos(List<ServerInfo> serverInfos) {
		this.serverInfos = serverInfos;
	}
}
